<table width="100%" cellspacing="2">
  <tr>
    <th style="text-align:left; font-weight:bold;">Registered at</th>
    <th style="text-align:left; font-weight:bold;">Name</th>
    <th style="text-align:left; font-weight:bold;">Bio</th>
    <th style="text-align:left; font-weight:bold;">Role</th>
    <th style="text-align:left; font-weight:bold;">Photo</th>
    <th style="text-align:left; font-weight:bold;">Order</th>
    <th style="text-align:left; font-weight:bold;">Advisor</th>
    <th style="text-align:left; font-weight:bold;">Team</th>
    <th style="text-align:left; font-weight:bold;">Author</th>
  </tr>

  <% @collection.each do |excel| %>
    <tr>
      <td style="text-align:left;"><%= excel.created_at.to_date %></td>
      <td style="text-align:left;"><%= excel.name %></td>
      <td style="text-align:left;"><%= excel.bio %></td>
      <td style="text-align:left;"><%= excel.role %></td>
      <td style="text-align:left;"><%= root_url+excel.photo.url %></td>
      <td style="text-align:left;"><%= excel.theorder %></td>
      <td style="text-align:left;"><%= excel.advisor %></td>
      <td style="text-align:left;"><%= excel.team %></td>
      <td style="text-align:left;"><%= excel.author %></td>
    </tr>
  <% end %>

</table>
