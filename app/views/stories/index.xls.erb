<table width="100%" cellspacing="2">
  <tr>
    <th style="text-align:left; font-weight:bold;">Registered at</th>
    <th style="text-align:left; font-weight:bold;">Title</th>
    <th style="text-align:left; font-weight:bold;">Brief</th>
    <th style="text-align:left; font-weight:bold;">Content</th>
    <th style="text-align:left; font-weight:bold;">External link</th>
    <th style="text-align:left; font-weight:bold;">Published</th>
  </tr>

  <% @collection.each do |excel| %>
    <tr>
      <td style="text-align:left;"><%= excel.created_at.to_date %></td>
      <td style="text-align:left;"><%= excel.title %></td>
      <td style="text-align:left;"><%= excel.brief %></td>
      <td style="text-align:left;"><%= excel.content %></td>
      <td style="text-align:left;"><%= excel.external_link %></td>
      <td style="text-align:left;"><%= excel.published %></td>
    </tr>
  <% end %>

</table>
