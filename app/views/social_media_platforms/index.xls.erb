<table width="100%" cellspacing="2">
  <tr>
    <th style="text-align:left; font-weight:bold;">Registered at</th>
    <th style="text-align:left; font-weight:bold;">title</th>
    <th style="text-align:left; font-weight:bold;">color</th>
    <th style="text-align:left; font-weight:bold;">font_awesome_style</th>
  </tr>

  <% @social_media_platforms.each do |excel| %>
    <tr>
      <td style="text-align:left;"><%= excel.created_at.to_date %></td>
      <td style="text-align:left;"><%= excel.title %></td>
      <td style="text-align:left;"><%= excel.color %></td>
      <td style="text-align:left;"><%= excel.font_awesome_style %></td>
    </tr>
  <% end %>

</table>